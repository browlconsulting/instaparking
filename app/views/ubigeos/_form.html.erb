<%= simple_form_for @ubigeo, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :district %>
  <%= error_span(@ubigeo[:district]) %>
  <%= f.input :department %>
  <%= error_span(@ubigeo[:department]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                ubigeos_path, :class => 'btn btn-default' %>
<% end %>
