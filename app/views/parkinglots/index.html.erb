<%- model_class = Parkinglot -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:address) %></th>
      <th><%= model_class.human_attribute_name(:addressreference) %></th>
      <th><%= model_class.human_attribute_name(:pricinghour) %></th>
      <th><%= model_class.human_attribute_name(:pricingday) %></th>
      <th><%= model_class.human_attribute_name(:businesshours) %></th>
      <th><%= model_class.human_attribute_name(:camera) %></th>
      <th><%= model_class.human_attribute_name(:security) %></th>
      <th><%= model_class.human_attribute_name(:user_id) %></th>
      <th><%= model_class.human_attribute_name(:ubigeo_id) %></th>
      <th><%= model_class.human_attribute_name(:nearby_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @parkinglots.each do |parkinglot| %>
      <tr>
        <td><%= link_to parkinglot.id, parkinglot_path(parkinglot) %></td>
        <td><%= parkinglot.name %></td>
        <td><%= parkinglot.address %></td>
        <td><%= parkinglot.addressreference %></td>
        <td><%= parkinglot.pricinghour %></td>
        <td><%= parkinglot.pricingday %></td>
        <td><%= parkinglot.businesshours %></td>
        <td><%= parkinglot.camera %></td>
        <td><%= parkinglot.security %></td>
        <td><%= parkinglot.user_id %></td>
        <td><%= parkinglot.ubigeo_id %></td>
        <td><%= parkinglot.nearby_id %></td>
        <td><%=l parkinglot.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_parkinglot_path(parkinglot), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      parkinglot_path(parkinglot),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_parkinglot_path,
            :class => 'btn btn-primary' %>
